<?xml version="1.0" encoding="UTF-8"?>
<se:FeatureTypeStyle xmlns="http://www.opengis.net/sld" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.opengis.net/sld http://schemas.opengis.net/sld/1.1/StyledLayerDescriptor.xsd" xmlns:se="http://www.opengis.net/se" xmlns:ogc="http://www.opengis.net/ogc" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:tds="http://metadata.dod.mil/mdr/ns/GSIP/4.0/tds/4.0">

<!-- FloatingDryDockGeosurface -->
			<se:FeatureTypeName>tds:FloatingDryDockGeosurface</se:FeatureTypeName>

				<!--Floating Dry Dock Surface Symbol-->
				<se:Rule>
					<se:PolygonSymbolizer>
						<se:Name>Black_0.2mmSolidLine</se:Name>
						<se:Stroke>
							<se:SvgParameter name="stroke">#000000</se:SvgParameter>
							<se:SvgParameter name="stroke-width">1</se:SvgParameter>
						</se:Stroke>
					</se:PolygonSymbolizer>
				</se:Rule>

				<!-- AR_Black_FloatingDryDock  / Usable Label-->
				<se:Rule>
				<se:Name>AR_Black_FloatingDryDock Usable</se:Name>
					<ogc:Filter xmlns:tds="http://metadata.dod.mil/mdr/ns/GSIP/4.0/tds/4.0">
						<ogc:Or>
							<ogc:PropertyIsEqualTo>
								<ogc:PropertyName>tds:physicalCondition</ogc:PropertyName>
								<ogc:Literal>intact</ogc:Literal>
							</ogc:PropertyIsEqualTo>
							<ogc:PropertyIsEqualTo>
								<ogc:PropertyName>tds:physicalCondition</ogc:PropertyName>
								<ogc:Literal>noInformation</ogc:Literal>
							</ogc:PropertyIsEqualTo>
						</ogc:Or>
					</ogc:Filter>
					<se:TextSymbolizer>
						<se:Label>
							<ogc:PropertyName>tds:geoNameCollection.memberGeoName.fullName</ogc:PropertyName>
							<ogc:Literal> Floating dry dock</ogc:Literal>
						</se:Label>
						<se:Font>
							<se:SvgParameter name="font-family">Arial Unicode MS</se:SvgParameter>
							<se:SvgParameter name="font-family">Arial</se:SvgParameter>
							<se:SvgParameter name="font-size">10</se:SvgParameter>
							<se:SvgParameter name="font-style">normal</se:SvgParameter>
						</se:Font>
							<se:LabelPlacement>
								<se:LinePlacement>
									<se:PerpendicularOffset>-4</se:PerpendicularOffset>
									<se:IsRepeated>false</se:IsRepeated>
								</se:LinePlacement>
							</se:LabelPlacement>
						<se:Fill>
							<se:SvgParameter name="fill">#000000</se:SvgParameter>
						</se:Fill>
					</se:TextSymbolizer>
				</se:Rule>

				<!-- AR_Black_FloatingDryDock / Construction label-->
				<se:Rule>
				<se:Name>AR_Black_FloatingDryDock Construction</se:Name>
					<ogc:Filter xmlns:tds="http://metadata.dod.mil/mdr/ns/GSIP/4.0/tds/4.0">
								<ogc:PropertyIsEqualTo>
									<ogc:PropertyName>tds:physicalCondition</ogc:PropertyName>
									<ogc:Literal>construction</ogc:Literal>
								</ogc:PropertyIsEqualTo>
					</ogc:Filter>
					<se:TextSymbolizer>
						<se:Label>
							<ogc:PropertyName>tds:geoNameCollection.memberGeoName.fullName</ogc:PropertyName>
							<ogc:Literal> Floating dry dock (construction)</ogc:Literal>
						</se:Label>
						<se:Font>
							<se:SvgParameter name="font-family">Arial Unicode MS</se:SvgParameter>
							<se:SvgParameter name="font-family">Arial</se:SvgParameter>
							<se:SvgParameter name="font-size">10</se:SvgParameter>
							<se:SvgParameter name="font-style">normal</se:SvgParameter>
						</se:Font>
							<se:LabelPlacement>
								<se:LinePlacement>
									<se:PerpendicularOffset>-4</se:PerpendicularOffset>
									<se:IsRepeated>false</se:IsRepeated>
								</se:LinePlacement>
							</se:LabelPlacement>
						<se:Fill>
							<se:SvgParameter name="fill">#000000</se:SvgParameter>
						</se:Fill>
					</se:TextSymbolizer>
				</se:Rule>

				<!--AR_Black_FloatingDryDock / Not Usable label-->
				<se:Rule>
				<se:Name>AR_Black_FloatingDryDock</se:Name>
					<ogc:Filter xmlns:tds="http://metadata.dod.mil/mdr/ns/GSIP/4.0/tds/4.0">
						<ogc:Or>
							<ogc:PropertyIsEqualTo>
								<ogc:PropertyName>tds:physicalCondition</ogc:PropertyName>
								<ogc:Literal>unmaintained</ogc:Literal>
							</ogc:PropertyIsEqualTo>
							<ogc:PropertyIsEqualTo>
								<ogc:PropertyName>tds:physicalCondition</ogc:PropertyName>
								<ogc:Literal>damaged</ogc:Literal>
							</ogc:PropertyIsEqualTo>
							<ogc:PropertyIsEqualTo>
								<ogc:PropertyName>tds:physicalCondition</ogc:PropertyName>
								<ogc:Literal>dismantled</ogc:Literal>
							</ogc:PropertyIsEqualTo>
							<ogc:PropertyIsEqualTo>
								<ogc:PropertyName>tds:physicalCondition</ogc:PropertyName>
								<ogc:Literal>destroyed</ogc:Literal>
							</ogc:PropertyIsEqualTo>
						</ogc:Or>
					</ogc:Filter>
					<se:TextSymbolizer>
						<se:Label>
							<ogc:PropertyName>tds:geoNameCollection.memberGeoName.fullName</ogc:PropertyName>
							<ogc:Literal> Floating dry dock (not usable)</ogc:Literal>
						</se:Label>
						<se:Font>
							<se:SvgParameter name="font-family">Arial Unicode MS</se:SvgParameter>
							<se:SvgParameter name="font-family">Arial</se:SvgParameter>
							<se:SvgParameter name="font-size">10</se:SvgParameter>
							<se:SvgParameter name="font-style">normal</se:SvgParameter>
						</se:Font>
							<se:LabelPlacement>
								<se:LinePlacement>
									<se:PerpendicularOffset>-4</se:PerpendicularOffset>
									<se:IsRepeated>false</se:IsRepeated>
								</se:LinePlacement>
							</se:LabelPlacement>
						<se:Fill>
							<se:SvgParameter name="fill">#000000</se:SvgParameter>
						</se:Fill>
					</se:TextSymbolizer>
				</se:Rule>
			</se:FeatureTypeStyle>
